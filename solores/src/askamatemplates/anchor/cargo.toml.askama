[package]
name = "{{ crate_name }}"
version = "0.1.0"
edition = "2021"
description = "Generated Solana program interface for {{ crate_name }}"

[features]
default = []
serde = ["dep:serde", "dep:serde_json"]

[dependencies]
borsh = "0.10"
solana-program = "1.17"
serde = { version = "1.0", features = ["derive"], optional = true }
serde_json = { version = "1.0", optional = true }